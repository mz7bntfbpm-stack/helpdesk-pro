# Environment variables for Firebase Functions
# Copy this to .env and fill in your values

# SendGrid Configuration (for emails)
sendgrid.api_key=SG.your_sendgrid_api_key_here
sendgrid.from_email=support@yourdomain.com

# Slack Configuration (for notifications)
slack.token=xoxb-your-slack-bot-token
slack.channel=#support-tickets

# App Configuration
app.url=https://your-app.firebaseapp.com

# Optional: Analytics
app.analytics_enabled=true
